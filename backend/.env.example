# Environment Variables Template
# Copy this file to .env and fill in your values

# Database
DATABASE_URL=postgresql://admin:changeme@localhost:5432/sat_discovery
DB_PASSWORD=changeme

# Redis
REDIS_URL=redis://localhost:6379

# Service URLs (for development)
DATA_INGESTION_URL=http://localhost:3001
ORBITAL_URL=http://localhost:3002
PROCESSING_URL=http://localhost:3003

# API Keys (Optional - for external services)
# NASA Earthdata
NASA_API_KEY=

# ESA Copernicus (for SciHub)
ESA_USERNAME=
ESA_PASSWORD=

# Google Earth Engine (if needed)
GEE_SERVICE_ACCOUNT=
GEE_PRIVATE_KEY=

# Service Ports
GATEWAY_PORT=3000
DATA_INGESTION_PORT=3001
ORBITAL_PORT=3002
PROCESSING_PORT=3003

# Application
NODE_ENV=development
LOG_LEVEL=info

# Task Queue
TASK_CONCURRENCY=5
TASK_TIMEOUT=300000

# Storage (Optional - for algorithm outputs)
STORAGE_TYPE=local
STORAGE_PATH=./data/outputs

# CORS
CORS_ORIGIN=http://localhost:8080,https://sat-discovery.vercel.app

# Rate Limiting
RATE_LIMIT_WINDOW=60000
RATE_LIMIT_MAX_REQUESTS=100
